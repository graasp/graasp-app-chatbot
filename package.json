  "type": "module",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@graasp/apps-query-client": "^3.7.3",
    "@graasp/sdk": "^5.5.0",
    "@graasp/stylis-plugin-rtl": "^2.2.0",
    "@graasp/ui": "5.4.7",
    "@mui/icons-material": "6.3.1",
    "@mui/lab": "6.0.0-beta.22",
    "@mui/material": "6.3.1",
    "@sentry/react": "8.48.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@types/node": "22.10.5",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/stopword": "^2.0.3",
    "@uiw/react-codemirror": "^4.23.7",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.0",
    "lodash.countby": "^4.6.0",
    "lodash.flatten": "^4.4.0",
    "lodash.groupby": "^4.6.0",
    "lucide-react": "^0.469.0",
    "prism-react-renderer": "^2.4.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.0",
    "react-markdown": "^9.0.3",
    "react-mde": "12.0.8",
    "react-router-dom": "^6.28.1",
    "react-toastify": "11.0.2",
    "react-wordcloud": "^1.2.7",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "stopword": "^3.1.4",
    "string-to-color": "^2.2.2",
    "typescript": "5.7.2"
  },
  "scripts": {
    "dev": "yarn vite",
    "start": "yarn dev",
    "start:test": "yarn vite --mode test",
    "build": "yarn vite build",
    "build:test": "yarn vite build --mode test",
    "preview": "yarn vite preview",
    "preview:test": "yarn vite preview --mode test",
    "postinstall": "husky install",
    "lint": "eslint .",
    "prettier:write": "prettier {src,cypress}/**/*.{ts,tsx,js,jsx} --write",
    "prettier:check": "prettier {src,cypress}/**/*.{ts,tsx,js,jsx} --check",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "hooks:install": "husky install",
    "hooks:uninstall": "husky uninstall",
    "pre-commit": "yarn prettier:check && yarn lint",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "test": "concurrently -k -s first \"yarn start:test\" \"yarn test:ci\" ",
    "test:ci": "env-cmd -f ./.env.test cypress run && nyc report --reporter=text --reporter=text-summary",
    "cov:report": "open ./coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@cypress/code-coverage": "3.13.10",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/i18n": "0.13.12",
    "@types/lodash.countby": "^4.6.9",
    "@types/lodash.flatten": "^4.4.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "1.7.9",
    "concurrently": "9.1.2",
    "cypress": "13.17.0",
    "env-cmd": "10.1.0",
    "eslint": "^9.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "4.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "nock": "^13.5.6",
    "nyc": "17.1.0",
    "prettier": "3.4.2",
    "uuid": "11.0.4",
    "vite": "^6.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "^2.1.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ]
  },
  "packageManager": "yarn@4.6.0",
  "msw": {
    "workerDirectory": "public"
  }
}
